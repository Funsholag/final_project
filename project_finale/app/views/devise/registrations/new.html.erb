<img id="dock184" alt="" class="background-image responsive-img"></img>
  <div class="border-form-div">
    <h2 class= "signInTitle">Sign Up</h2>
    <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
      <%= f.email_field :email, class: "form-control center responsive", placeholder: "Email", :autofocus => true %>
      <%= f.password_field :password, class: "form-control center responsive", placeholder: "Password" %>
      <% if devise_mapping.rememberable? -%>
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      <% end -%>
      <span class= "signInBtn">
        <%= f.submit "Sign Up"%>
      </span>
        <% end %>
    <%= render "devise/shared/links" %>
  </div>
