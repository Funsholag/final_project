  <source src="722675159.mp4" type="video/mp4">
</video>

<h2 class= 'propBox'>Get Your Estimate</h2>
<div id="zillow-container">
<%= form_for @response.body, :url => property_finder_path do |f| %>
  <div class= "addressContainer">
    <%= text_field_tag(:address, params[:address], class: "form-control center responsive", placeholder: "Address (e.g 123 Example Street, City, State)", style: "height: 100%")%> <br />
  </div>
  <div class= 'zipcodeContainer'>
    <%= text_field_tag(:citystatezip, params[:citystatezip], class: "form-control center responsive", placeholder: "Zip Code", style: "height: 100%") %> <br />
  </div>
  <%= submit_tag "Submit", :class => "btn btn-default" %>
</div>

<% end %>
